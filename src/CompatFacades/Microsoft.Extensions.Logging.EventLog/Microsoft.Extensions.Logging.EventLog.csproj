<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Description>Windows Event Log logger provider implementation for Microsoft.Extensions.Logging.</Description>
    <TargetFrameworks>netstandard2.0;net461</TargetFrameworks>
    <NoWarn>$(NoWarn);CS1591</NoWarn>
    <PackageTags>$(PackageTags);eventlog;windowseventlog</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\Microsoft.Extensions.Logging\Microsoft.Extensions.Logging.csproj" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="System.Diagnostics.EventLog" Version="$(SystemDiagnosticsEventLogPackageVersion)" />
  </ItemGroup>

</Project>
